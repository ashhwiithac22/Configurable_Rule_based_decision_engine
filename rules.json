{
  "rules": [
    {
      "id": "review_rule",
      "decision": "REVIEW",
      "logic": "AND",
      "conditions": [
        {
          "field": "amount",
          "operator": ">",
          "value": 50000
        },
        {
          "field": "risk_score",
          "operator": ">",
          "value": 0.7
        }
      ]
    },
    {
      "id": "approve_rule",
      "decision": "APPROVE",
      "logic": "AND",
      "conditions": [
        {
          "field": "vendor_type",
          "operator": "==",
          "value": "Trusted"
        },
        {
          "field": "amount",
          "operator": "<",
          "value": 200000
        }
      ]
    },
    {
      "id": "high_risk_rule",
      "decision": "REVIEW",
      "logic": "OR",
      "conditions": [
        {
          "field": "country",
          "operator": "IN",
          "value": ["Nigeria", "Brazil", "Indonesia", "Philippines", "Myanmar", "China"]
        },
        {
          "field": "risk_score",
          "operator": ">",
          "value": 0.9
        }
      ]
    },
    {
      "id": "nested_rule",
      "decision": "REVIEW",
      "logic": "AND",
      "conditions": [
        {
          "field": "amount",
          "operator": ">",
          "value": 40000
        },
        {
          "group": {
            "logic": "OR",
            "conditions": [
              {
                "field": "risk_score",
                "operator": ">",
                "value": 0.85
              },
              {
                "field": "country",
                "operator": "IN",
                "value": ["Nigeria", "Brazil", "Indonesia", "Philippines", "Myanmar", "China"]
              }
            ]
          }
        }
      ]
    },
    {
      "id": "low_risk_approval",
      "decision": "APPROVE",
      "logic": "AND",
      "conditions": [
        {
          "field": "amount",
          "operator": "<=",
          "value": 10000
        },
        {
          "field": "risk_score",
          "operator": "<=",
          "value": 0.3
        },
        {
          "field": "vendor_type",
          "operator": "!=",
          "value": "Blacklisted"
        }
      ]
    },
    {
      "id": "very_high_risk",
      "decision": "REJECT",
      "logic": "OR",
      "conditions": [
        {
          "field": "amount",
          "operator": ">=",
          "value": 1000000
        },
        {
          "field": "risk_score",
          "operator": ">=",
          "value": 0.95
        },
        {
          "field": "vendor_type",
          "operator": "==",
          "value": "Blacklisted"
        }
      ]
    },
    {
      "id": "suspicious",
      "decision": "REVIEW",
      "logic": "AND",
      "conditions": [
        {
          "field": "amount",
          "operator": ">",
          "value": 20000
        },
        {
          "field": "amount",
          "operator": "<",
          "value": 50000
        },
        {
          "group": {
            "logic": "AND",
            "conditions": [
              {
                "field": "previous_orders",
                "operator": "<",
                "value": 3
              },
              {
                "field": "country",
                "operator": "NOT IN",
                "value": ["US", "UK", "Canada", "Australia"]
              }
            ]
          }
        }
      ]
    },
    {
      "id": "complex_nested",
      "decision": "REVIEW",
      "logic": "AND",
      "conditions": [
        {
          "group": {
            "logic": "OR",
            "conditions": [
              {
                "field": "amount",
                "operator": ">",
                "value": 100000
              },
              {
                "field": "country",
                "operator": "IN",
                "value": ["Nigeria", "Brazil", "Indonesia"]
              }
            ]
          }
        },
        {
          "group": {
            "logic": "AND",
            "conditions": [
              {
                "field": "risk_score",
                "operator": ">",
                "value": 0.5
              },
              {
                "field": "vendor_type",
                "operator": "NOT IN",
                "value": ["Trusted", "Verified"]
              }
            ]
          }
        }
      ]
    },
    {
      "id": "age_verification",
      "decision": "REJECT",
      "logic": "OR",
      "conditions": [
        {
          "field": "customer_age",
          "operator": "<",
          "value": 18
        },
        {
          "field": "customer_age",
          "operator": ">",
          "value": 65
        }
      ]
    }
  ]
}